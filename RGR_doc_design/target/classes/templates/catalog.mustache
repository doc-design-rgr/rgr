<html>
<body>
<h3>Hello</h3>

<form method="get" action="getEvent">
    <h3>Event</h3>
    <button type="submit">Search</button>

    <label for="cars">Choose a car:</label>

    <select name="cars" id="cars" >
        {{#event}}
            <option value="{{bouquetEvent}}">{{bouquetEvent}}</option>
        {{/event}}
    </select>
</form>


<div>
    {{#customBouquet}}
        <div>
            <b>{{bouquetId}}</b>
            <b>{{bouquetName}}</b>

            <b>{{price}}</b>
            <b>{{bouquetEvent}}</b>
        </div>
    {{/customBouquet}}
</div>
</body>
</html>